    // Primera clase: Point
    class Point
        x: float;
        y: float;
        
        func init(self: Point, x: float, y: float) -> void
            self.x = x;
            self.y = y;
        end
        
        func distance(self: Point, other: Point) -> float
            dx = self.x - other.x;
            dy = self.y - other.y;
            return sqrt(dx * dx + dy * dy);
        end
    end
    
